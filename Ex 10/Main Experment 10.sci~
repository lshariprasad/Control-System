//Root locus 
clear; 
clc; 
xdel(winsid()); 
 
//cd"your directory" 
cd"D:\Control System\Lab\root1.sci"
exec("root1.sci"); 

s=%s; 
G=syslin('c',1,s*(s+1)*(s^2+4*s+13)); 
disp(G, 'The given transfer function G(s)='); 
root1(G,[-6 -5;6 5],'Root locus plot for the given transfer function G(s)=1/s(s+1)(s^2+4s+13)'); 
